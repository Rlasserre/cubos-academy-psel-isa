function solucao(velocidade, aceleracao, distanciaObjeto) {
  const distanciaFrenagem = -1 * (velocidade * velocidade) / ( 2 * aceleracao);
    
    if (distanciaFrenagem > distanciaObjeto) {
        
        console.log("COLISAO A FRENTE");
    
    } else if ( distanciaFrenagem === distanciaObjeto){
    
        console.log("NAO ACELERE");
    
    } else {
        console.log("CAMINHO SEGURO");
    }
    
    
}
    
